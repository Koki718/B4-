	// 		"console.log('$1');",
	// 		"$2"
	// 	],
	// 	"description": "Log output to console"
	// }
	"Format": {
			"prefix": "report",
			"body": [
				"\\documentclass[platex,dvipdfmx]{jlreq}",
				"\\usepackage{graphicx}",
				"\\usepackage{bm}",
				"\\usepackage{url}",
				"\\usepackage{tikz}",
				"\\usepackage{longtable}",
				"\\usepackage{siunitx}",
				"\\usepackage{amsmath}",
				"\\begin{document}",
				"",
				"\\end{document}",
				
			],
			"description": "LaTeXテンプレート"
	}
	,
	"Graph": {
		"prefix": "includegraphics",
		"body": [
			"\\begin{figure}[ht]",
			"\\centering",
			"\\includegraphics[width=100mm]{}",
			"\\caption{}",
			"\\label{}",
			"\\end{figure}",
		],
		"description": "Graphテンプレート"
	},
	"Table": {
		"prefix": "tabular",
		"body": [
			"\\begin{table}[ht]",
			"\\centering",
			"\\caption{}",
			"\\begin{tabular}{}",
			"",
			"",
			"\\end{tabular}",
			"\\label{}",
			"\\end{table}",
		],
		"description": "Tableテンプレート"
	},

	}
